{
  "name": "callcenter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc -p tsconfig.check.json",
    "build": "yarn typecheck && parcel build --target cmd --no-cache --no-optimize"
  },
  "targets": {
    "cmd": {
      "context": "node",
      "source": [
        "src/index.ts"
      ],
      "outputFormat": "commonjs",
      "sourceMap": false
    }
  },
  "devDependencies": {
    "parcel": "^2.8.3",
    "typescript": "^4.9.5"
  }
}
